{
    "manifest_version": 3,
    "name": "eLitmus Proctoring Extension",
    "version": "0.0.1",
    "description": "eLitmus  Proctoring Extension",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions":[
        "scripting",
        "tabs",
        "activeTab",
        "storage"
    ],
    "host_permissions": [
        "*://*/*"
    ]
}